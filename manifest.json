{
  "name": "JIRA Worklog Helper",
  "version": "1.0",
  "description": "Makes tracking and logging time easy for JIRA users.",
  "background": {
    "scripts": ["eventPage.js"]
  },
  "browser_action" :
  {
    "default_icon" : "off-19.png",
    "default_title" : "Click to toggle the timer."
  },
  "permissions" : [
    "storage",
    "activeTab"
  ],
  "icons" : {
    "48" : "off-48.png",
    "128" : "off-128.png"
  },
  "manifest_version": 2
}

